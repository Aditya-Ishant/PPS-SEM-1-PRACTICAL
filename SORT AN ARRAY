#include <stdio.h>

void main() {
    int a[6]= {12, 5, 10, 9, 7, 6};
    int temp;
    int i, j;

    // Display array before sorting
    printf("Before Sorting: ");
    for(i=0; i<6; i++) {
        printf("%d ", a[i]);
    }
    printf("\n");

    // Bubble sort algorithm
    for(i=0; i<5; i++) {  // Only need to go up to the second-to-last element
        for(j=0; j<5-i; j++) {  // Elements after 5-i are already sorted
            if(a[j] > a[j+1]) {  // Compare adjacent elements
                temp = a[j];
                a[j] = a[j+1];
                a[j+1] = temp;
            }
        }
    }

    // Display array after sorting
    printf("After Sorting: ");
    for(i=0; i<6; i++) {
        printf("%d ", a[i]);
    }
    printf("\n");
}
