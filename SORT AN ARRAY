#include <stdio.h>

void main() {
    int a[6]= {12, 5, 10, 9, 7, 6};
    int temp;
    int i, j;


    printf("Before Sorting: ");
    for(i=0; i<6; i++) {
        printf("%d ", a[i]);
    }
    printf("\n");

    // Bubble sort algorithm
    for(i=0; i<5; i++) {  
        for(j=0; j<5-i; j++) {  
            if(a[j] > a[j+1]) {  
                temp = a[j];
                a[j] = a[j+1];
                a[j+1] = temp;
            }
        }
    }

    
    printf("After Sorting: ");
    for(i=0; i<6; i++) {
        printf("%d ", a[i]);
    }
    printf("\n");
}
