
void main() {
    int a[] = {10, 20, 30, 40, 50};
    int *ptr;
    int *cpt[5]; // Array of pointers

    for (int i = 0; i < 5; i++) {
        cpt[i] = &a[i];
    }

    ptr = a;

    printf("Address of the first element of array: %p\n", (void *)ptr);
    printf("Value of the first element of array: %d\n", *ptr);

    printf("Values using the array of pointers:\n");
    for (int i = 0; i < 5; i++) {
        printf("%d\t", *cpt[i]);
    }
    printf("\n");
}
